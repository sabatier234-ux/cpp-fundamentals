#include <iostream>
#include <string>
using namespace std;
string removeDuplicate(const string& s) {
	string newword = " ";
	for (int i = 0; i < s.length(); i++) {
		bool duplicate = false;
		for (int j = 0; j < i; j++) {
			if (s[i] == s[j]) {
				duplicate = true;
			}
			
			}
			if (!duplicate) {
				newword += s[i];

		}
	}
	return newword;
}

int main() {
	string word;
	cout << "enter the word:";
	getline(cin, word);
	string newword = removeDuplicate(word);
	cout << "the new word after removing duplicates is:" << newword << endl;

	system("pause");
	return 0;
}
